#include <bits/stdc++.h>
using namespace std;
#define ll long long int
string isPalindrome(string s)
{
    
    string req = s;
    reverse(req.begin(), req.end());
    if (s == req) {
        return "Yes";
    }
    else {
        return "No";
    }
}

int main()
{
    string s;
    cin>>s;
    cout << isPalindrome(s);
 
    return 0;
}
